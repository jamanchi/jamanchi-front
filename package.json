    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "msw": "^1.2.2",
    "prettier": "2.8.8",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vitest": "^0.33.0"
